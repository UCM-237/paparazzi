      <call_once fun="NavSetWaypointHere(WP_CLIMB)"/>
      <stay vmode="climb" climb="nav.climb_vspeed" wp="CLIMB"/>
    </block>
    <block group="waypoint_control_col1" name="Standby" strip_button="Standby" strip_icon="standby.png">
